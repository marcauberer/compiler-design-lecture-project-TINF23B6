# Function def
double getDbl := (double x) {
    ret 4.567 + x;
}

int main := () {
    int i = 3;

    # While loop
    while (i == 3) {
        i = i - 1;
    }

    # Do-while loop
    do {
        i = i * 2;
    } while (i != 8);

    # For loop
    for (int j = 0; i != 10; i = i + 1) {
        i = i + 1;
    }

    # Switch/case/default
    switch (i) {
        case 1:
            i = i + 2;
        case 2:
            i = i / 2;
        default:
            i = i - 3;
    }

    # Anonymous block
    {
        int j = i + 2;
        i = j;
    }

    # Function call
    double d = call getDbl(5.42);
    print(d);

    ret 0;
}